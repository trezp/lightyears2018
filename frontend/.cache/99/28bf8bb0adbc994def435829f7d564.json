(card.id);\n      }\n    }\n  },\n  computed: {\n    hand: function() {\n      const hand = [];\n      this.deck.deck.forEach(card => {\n        if (card.inHand) {\n          hand.push(card);\n        }\n      });\n      return hand;\n    }\n  },\n  mounted() {\n    axios\n      .get(\"http://localhost:3000\")\n      .then(response => {\n        this.gameId = response.data.gameId;\n        this.players = response.data.players;\n        this.deck = response.data.deck;\n      })\n      .then(() => this.dealHand())\n      .catch(err => console.log(err));\n  }\n});\n</script>\n\n<style lang=\"scss\">\nbody {\n  font-family: \"Source Sans Pro\", sans-serif;\n}\nul {\n  list-style-type: none;\n  padding-left: 0;\n}\n\n.hand {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-around;\n\n  .card {\n    border: 1px solid black;\n    padding: 1%;\n    margin: 1%;\n    font-size: 1.5em;\n    text-align: center;\n    flex-basis: 20%;\n\n    span {\n      font-weight: bold;\n      font-size: 2em;\n    }\n  }\n}\n</style>"},"lineCount":null}},"error":null,"hash":"447593cdb04e6aaa0d9e242594ead1b3","cacheData":{"env":{}}}